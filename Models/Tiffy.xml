 <!-- ********************** Animations ************************* -->

 <animation>
  <type>select</type>
  <object-name>LTire</object-name>
  <object-name>LMainGearStrut</object-name>
  <object-name>LGearDoorOuter</object-name>
  <object-name>LGearDoorInner</object-name>
  <condition>
   <greater-than>
    <property>gear/gear[0]/position-norm</property>
    <value>0</value>
   </greater-than>
  </condition>
 </animation>

 <animation>
  <type>select</type>
  <object-name>RTire</object-name>
  <object-name>RMainGearStrut</object-name>
  <object-name>RGearDoorOuter</object-name>
  <object-name>RGearDoorInner</object-name>
  <condition>
   <greater-than>
    <property>gear/gear[1]/position-norm</property>
    <value>0</value>
   </greater-than>
  </condition>
 </animation>

 <animation>
  <type>select</type>
  <object-name>LMainGearBrace</object-name>
  <object-name>RMainGearBrace</object-name>
  <condition>
   <greater-than>
    <property>gear/gear[0]/position-norm</property>
    <value>0.9</value>
   </greater-than>
  </condition>
 </animation>

 <animation>
  <type>select</type>
  <object-name>LWheelWellCover</object-name>
  <object-name>RWheelWellCover</object-name>
  <condition>
   <equals>
    <property>gear/gear[0]/position-norm</property>
    <value>0</value>
   </equals>
  </condition>
 </animation>

 <animation>
  <type>rotate</type>
  <object-name>TailGearArm</object-name>
  <object-name>Tailwheel</object-name>
  <property>sim/multiplay/generic/float[2]</property>
  <factor>-1</factor>
  <center>
   <x-m> 5.60</x-m>
   <y-m> 0.00</y-m>
   <z-m>-0.61</z-m>
  </center>
  <axis>
   <x>0</x>
   <y>0</y>
   <z>1</z>
  </axis>
 </animation>

 <animation>
  <type>rotate</type>
  <object-name>TailwheelStrut</object-name>
  <object-name>Tailwheel</object-name>
  <object-name>TailGearArm</object-name>
  <property>gear/gear[0]/position-norm</property>
  <factor>65</factor>
  <offset-deg>-65</offset-deg>   
  <center>
   <x-m> 5.87</x-m>
   <y-m>    0</y-m>
   <z-m>-0.34</z-m>
  </center>
  <axis>
   <x>0</x>
   <y>-1</y>
   <z>0</z>
  </axis>
 </animation>

 <animation>
  <type>rotate</type>
  <object-name>LTire</object-name>
  <object-name>LMainGearStrut</object-name>
  <object-name>LGearDoorOuter</object-name>
  <object-name>LMainGearBrace</object-name>
  <property>gear/gear[0]/position-norm</property>
  <interpolation>
    <entry><ind>0.2</ind><dep>-97</dep></entry>
    <entry><ind>1</ind><dep>0</dep></entry>
  </interpolation>  
  <center>
   <x-m>-0.35</x-m>
   <y-m>-2.38</y-m>
   <z-m>-0.83</z-m>
  </center>
  <axis>
   <x>-1</x>
   <y>0.53</y>
   <z>0</z>
  </axis>
 </animation>

 <animation><!-- rotate main gear to fit flat into wing -->
  <type>rotate</type>
